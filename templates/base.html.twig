<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
         <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body class="{% block body_class %}hold-transition sidebar-mini layout-fixed layout-navbar-fixed layout-footer-fixed{% endblock %}">
  
        <div class="wrapper">
            {% block header %}
                {% include 'components/nav.html.twig' %}
                {% include 'components/aside.html.twig' %}
            {% endblock %}
            {% block body %} {% endblock %}
            {% block footer %}
                {% include 'components/footer.html.twig' %}
             {% endblock %}

        </div>
        {% block javascripts %}
        
        {{ encore_entry_script_tags('app') }} 
        {% endblock %}
    </body>
</html>
